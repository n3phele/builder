options:
  plugins:
    type: string
    default: ""
    description: Space separated list of plugins to install
  job:
    type: string
    default: ""
    description: Name of job
  builder:
    type: string
    default: ""
    description: git repository with contains script and config.xml to create job and dependencies.
  configName:
    type: string
    default: "config.xml"
    description: Name of .xml file to create a job. 
